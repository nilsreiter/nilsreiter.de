{% for t in site.data.talks %}

{% if site.data.locations[t.institution] %}
{% assign inst = site.data.locations[t.institution] %}
{% else %}
{% assign inst = t.institution %}
{% endif %}

@misc{ {{ "talk-" | append: t.date | slugify: "ascii" }},
  title = { {{ t.title|replace: "&", "\\&" }} },
  author = { {% for a in t.with %}{{a}} and {% endfor %}Nils Reiter },
  date = { {{ t.date }} },
  {% if t.venue.label %}note = { {{ t.venue.label|replace: "&", "\\&" }} },{% endif %}
  type = { Talk },
  keywords = { talk },
  location = { {{ inst.name|replace: "&", "\\&" }} }
}

{% endfor %}